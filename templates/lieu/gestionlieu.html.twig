{% extends 'base.html.twig' %}
{% block stylesheets %} <link rel="stylesheet" href="{{ asset('css/form.css') }}">{% endblock %}

{% block body %}
<div>
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <select id="ville" name="ville">
        {% for ville in villes %}
            <option name="ville" value="{{ ville.id }}">{{ ville.nom }}</option>
        {% endfor %}
    </select>
    <button>Créer</button>
    {{ form_end(form) }}
</div>
{% endblock %}

{% block title %}
Créer lieu
{% endblock %}

